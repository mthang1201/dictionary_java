<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="480" prefWidth="700"
            xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.uet.dictionary_java.controllers.EditController">
    <ScrollPane fx:id="scrollPane" prefHeight="480" prefWidth="700">
        <GridPane fx:id="gridPane" hgap="10" vgap="10" />
    </ScrollPane>
    <Button text="-" onAction="#decreasePage" />
    <Button fx:id="pageBtn" />
    <Button text="-" onAction="#increasePage" />
</AnchorPane>
